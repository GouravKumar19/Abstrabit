generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Bookmark {
  id        String   @id @default(uuid())
  title     String
  url       String
  userId    String   @map("user_id")
  createdAt DateTime @default(now()) @map("created_at")

  @@map("bookmarks")
}
